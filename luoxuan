#include<stdio.h>
	int main(){
		int N;
		scanf("%d",&N);
		while (N--){
			int n,m;
			scanf("%d %d",&n,&m);
			int kirby[n][m];
			int i = 1;
			int a = 1,b = 1;
			int tag = 0;
			int flag1 = 1,flag2 = 0,flag3 = 0,flag4 = 0;
			while (i != n * m + 1){
				if (flag1 == 1){
					kirby[a - 1][b - 1] = i++;
					if (b == m - 1 - tag){
						flag1 = 0;
						flag2 = 1;
					}
					b++;
					continue;
				}
				if (flag2 == 1){
					kirby[a - 1][b - 1] = i++;
					if (a == n - 1 - tag){
						flag2 = 0;
						flag3 = 1;
					} 
					a++;
					continue;
				}
				if (flag3 == 1){
					kirby[a - 1][b - 1] = i++;
					if (b == 2 + tag){
						flag3 = 0;
						flag4 = 1;
						tag++;
					}
					b--;
					continue;
				}
				if (flag4 == 1){
					kirby[a - 1][b - 1] = i++;
					if (a == 2 + tag){
						flag4 = 0;
						flag1 = 1;
//						tag++;
					}
					a--;
					continue;
				}
			}
			for (int i = 0;i < n;i++){
				for (int j = 0;j < m;j++){
					printf("%2d%c",kirby[i][j],j==m-1?10:' ');
				}
			}
		}
		return 0;
	}
